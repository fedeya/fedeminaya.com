{
  "name": "web",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "generate:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public --kv content"
  },
  "dependencies": {
    "@balavishnuvj/remix-seo": "^1.0.2",
    "@portabletext/react": "^2.0.2",
    "@remix-run/cloudflare": "^1.14.0",
    "@remix-run/cloudflare-pages": "^1.14.0",
    "@remix-run/react": "^1.14.0",
    "@sanity/image-url": "^1.0.2",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "fathom-client": "^3.5.0",
    "groq": "^3.6.0",
    "isbot": "^3.6.6",
    "picosanity": "^4.0.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-wrap-balancer": "^0.4.0",
    "remix-utils": "^6.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.14.0",
    "@remix-run/eslint-config": "^1.14.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.23.1",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.7.4",
    "wrangler": "^2.12.0"
  },
  "engines": {
    "node": ">=14"
  }
}
